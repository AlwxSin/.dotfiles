[user]
	name = Alwx
	email = alwxsin@gmail.com
[core]
	autocrlf = input
	excludesfile = /Users/Alwx/.gitignore
	editor = nano
[alias]
    lg = log --graph --pretty=format:'%Cred%h%Creset%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
    st = status -sb
    co = checkout
    ci = commit
    br = branch
[help]
    autocorrect = 1
[push]
    default = simple